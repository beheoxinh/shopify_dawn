<div class="tabs-layout">
    <div class="tab-header">
        <ul class="tab-head-list">
            <li>Shipping</li>
            <li class="selected">Discount</li>
        </ul>
    </div>
    <div class="tab-content">
        <div class="tab-section-content" id="tab1">
            <label>
                <select name="quantity">
                    <option>Standard shipping</option>
                    <option>Fast shipping</option>
                </select>
            </label>
            <div class="shipping-render">
                <ul class="shipping-chain">
                    <li class="chain-node">
                        <p class="process-title">Order Date</p>
                        <img id="cart-process" src="" alt="error">
                        <p class="process-date">20/08</p>
                    </li>
                    <li class="chain-line">
                        <hr>
                    </li>
                    <li class="chain-node">
                        <p class="process-title">Delivery Date</p>
                        <img id="shipping-process" src="" alt="error">
                        <p class="process-date">22/08-24/08</p>
                    </li>
                </ul>
            </div>
        </div>
        <div style="display: none" class="tab-section-content" id="tab2">
            <label>
                <select name="quantity">
                    <option>Buy 1 item</option>
                    <option>Buy 2 items</option>
                </select>
            </label>
            <table>
                <tr>
                    <th>Quantity</th>
                    <th>Price</th>
                </tr>
                <tr>
                    <td>Buy 1 item</td>
                    <td>$10</td>
                </tr>
            </table>
        </div>
    </div>
